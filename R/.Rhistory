gfci_graph <- function(gfci) {
edges <- gfci$edges
nodes <- gfci$nodes
rEG <- new("graphNEL", nodes=gfci$nodes, edgemode="directed");
for (edge in edges) {
edgeNodes <- strsplit(edge, " ")[[1]][-2]
rEG <- addEdge(edgeNodes, rEG, 1)
}
plot(rEG)
}
